<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.login.Register_user"></activity>
        <activity android:name=".ui.login.Login_user" />
        <activity android:name=".ui.login.Begin_user" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--   activity for setting  -->
        <activity android:name=".ui.setting.changelanguage_user" />
        <activity android:name=".ui.setting.tutorial_user" />
        <activity android:name=".ui.setting.changelayout_user" />
        <activity android:name=".ui.setting.rule_user" />
        <activity android:name=".ui.setting.achievement_user" />
        <activity android:name=".ui.setting.feedback_user" />
    </application>

</manifest>